REGEDIT4

[HKEY_CLASSES_ROOT\.P2kc]
@="P2kCommander"

[HKEY_CLASSES_ROOT\P2kCommander]
@="P2kCommander batchfile"

[HKEY_CLASSES_ROOT\P2kCommander\shell]

[HKEY_CLASSES_ROOT\P2kCommander\shell\open]
@=""

[HKEY_CLASSES_ROOT\P2kCommander\shell\open\command]
@="P2kCommander.exe \"%1\""

[HKEY_CLASSES_ROOT\P2kCommander\DefaultIcon]
@="P2kCommander.exe,1"